<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTabPane?>
<?import com.jfoenix.controls.JFXTextArea?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<JFXTabPane fx:id="requestTabPane" stylesheets="@../css/fullpackstyling.css" xmlns="http://javafx.com/javafx/8.0.371" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.phoenixx.ui.controllers.components.RequestDataController">
   <tabs>
      <Tab fx:id="tabNameLabel" disable="true" styleClass="TabNameLabel" text="Request">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
      <Tab text="Headers">
         <content>
            <HBox>
               <children>
                  <JFXTreeTableView fx:id="headersTable" prefHeight="625.0" prefWidth="999.0" stylesheets="@../css/fullpackstyling.css" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </content>
      </Tab>
      <Tab text="Cookies">
         <content>
            <HBox>
               <children>
                  <JFXTreeTableView fx:id="cookiesTable" prefHeight="625.0" prefWidth="999.0" stylesheets="@../css/fullpackstyling.css" HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
         </content>
      </Tab>
      <Tab text="Body">
         <content>
            <HBox>
               <children>
                  <JFXTextArea fx:id="bodyArea" styleClass="BodyTextArea" stylesheets="@../css/fullpackstyling.css" text="test" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <font>
                        <Font size="13.0" />
                     </font>
                  </JFXTextArea>
               </children>
            </HBox>
         </content>
      </Tab>
      <Tab text="Analysis View">
        <content>
           <HBox>
              <children>
                 <JFXTextArea fx:id="analysisTextArea" styleClass="BodyTextArea" stylesheets="@../css/fullpackstyling.css" text="test" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                       <Insets />
                    </HBox.margin>
                    <font>
                       <Font size="13.0" />
                    </font>
                 </JFXTextArea>
              </children>
           </HBox>
        </content>
      </Tab>
   </tabs>
</JFXTabPane>
