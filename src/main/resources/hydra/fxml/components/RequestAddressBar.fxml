<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox prefHeight="67.0" prefWidth="999.0" style="-fx-background-color: #2a3139;" xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.phoenixx.ui.controllers.components.RequestAddressBarController">
   <children>
      <JFXComboBox fx:id="requestTypeButton" minWidth="80.0" prefHeight="40.0" prefWidth="100.0" promptText="GET" style="-fx-background-color: #22272e;" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="10.0" top="10.0" />
         </HBox.margin>
      </JFXComboBox>
      <JFXTextField fx:id="addressBar" prefHeight="40.0" prefWidth="756.0" promptText="Test" style="-fx-background-color: #22272e; -fx-text-fill: white;" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets left="10.0" right="10.0" top="10.0" />
         </HBox.margin>
         <font>
            <Font size="16.0" />
         </font>
      </JFXTextField>
      <JFXButton fx:id="sendButton" minWidth="70.0" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #22272e;" text="SEND" textFill="#0ed300" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="10.0" top="10.0" />
         </HBox.margin>
      </JFXButton>
   </children>
</HBox>
